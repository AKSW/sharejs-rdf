block sharejs-config
  #sharejs-config.row
    .col-sm-1
      label(for="sharejs-server") Server
    .col-sm-8
      input#sharejs-server(type="url", value="http://localhost:4000", ng-model="serverUrl")

    .col-sm-3
      input#sharejs-connect(type="button", value="Connect >", ng-click="connect()")
